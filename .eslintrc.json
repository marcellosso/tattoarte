{
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": 1,
    "@typescript-eslint/no-non-null-asserted-optional-chain": 1,
    "react/no-unescaped-entities": 1,
    "semi": [1, "always"]
  }
}
